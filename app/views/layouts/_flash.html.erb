<% if flash[:notice] %>
  <div class="alert alert-success alert-dismisable text-center alert-custom" role="alert">
    <%= flash[:notice] %>
  </div>
<% end %>

<% if flash[:alert] %>
  <div class="alert alert-danger alert-dismisable text-center alert-custom" role="alert">
    <%= flash[:alert] %>
  </div>
<% end %>
