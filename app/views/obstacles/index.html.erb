<div class="row">
<h1>Obstacles</h1> <hr>
  <div class="column">
    <table class="table table-striped table-hover ">
      <thead>
        <tr>
          <th>Name </th>
          <th>Description</th>
          <th>Points</th>
        </tr>
      </thead>
      <tbody>
        <% @obstacles.each do |obstacle| %>
            <tr data-link="<%= action_path(action) %>">
                <td> <%= obstacle.name %></td>
                <td> <%= obstacle.description %></td>
                <td> <%= obstacle.bliss_pts %></td>
            </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>
